<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1</TargetFrameworks>
    <Nullable>enable</Nullable>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <Description>Nfantom - JsonRpc WebSocketClient</Description>
	  <AssemblyTitle>Nfantom.JsonRpc.WebSocketClient</AssemblyTitle>
	  <AssemblyName>Nfantom.JsonRpc.WebSocketClient</AssemblyName>
	  <PackageId>Nfantom.JsonRpc.WebSocketClient</PackageId>
